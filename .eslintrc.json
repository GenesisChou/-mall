{
    "extends": ["vue" /* your other extends */ ],
    "plugins": ["vue"],
    "globals": {
        "document": false,
        "navigator": false,
        "window": false,
        "APP": false,
        "utils": false,
        "location":false,
        "sessionStorage":false,
        "localStorage":false
    },
    "rules": {
        "semi": ["warn", "always"],
        "indent": ["warn", 4],
        "eol-last": "off",
        "space-before-function-paren": "off",
        "no-undef": "error",
        "camelcase": "off",
        "comma-dangle": "off",
        "spaced-comment": "off",
        "one-var":["warn","always"]
    }
}